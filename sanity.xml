	<!-- <groups>
	<run>
	  <include name="regression"/>
	</run>
	</groups>  -->
	<parameter name="browserType" value="firefox"/>
		<classes>
		<class name="com.cbs.testsuite.CBS_TC_SUITE_Test">
		<method>
				<include name="testNav"/> 
				<include name="menuItemListsTest"/>
			</method> 
		</class>
		</classes>
